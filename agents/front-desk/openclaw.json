{
    "agent": {
        "model": "qwenapi/qwen3.5-flash",
        "workspace": "."
    },
    "talk": {
        "ttsEngine": "coqui",
        "baseUrl": "http://127.0.0.1:8000",
        "endpoint": "/v1/text-to-speech",
        "voiceId": "default",
        "outputFormat": "mp3_44100_128",
        "interruptOnSpeech": true,
        "timeoutMs": 5000
    },
    "fallback": {
        "enabled": true,
        "ttsProvider": "elevenlabs",
        "apiKey": "sk-xxxxx",
        "voiceId": "pNInz6obpgDQGcFmaJgB"
    },
    "voiceWake": {
        "enabled": true,
        "keywords": ["小瑞", "你好", "前台"]
    },
    "skills": {
        "load": {
            "watch": true,
            "watchDebounceMs": 250
        }
    },
    "tools": {
        "camera": {
            "enabled": true,
            "device": "default",
            "resolution": "1920x1080"
        },
        "browser": {
            "enabled": false
        }
    },
    "server": {
        "port": 3000
    }
}
