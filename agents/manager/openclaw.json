{
    "agent": {
        "model": "qwenapi/qwen3.5-flash",
        "workspace": "."
    },
    "channels": {
        "dingtalk": {
            "enabled": true,
            "appKey": "${DINGTALK_APP_KEY}",
            "appSecret": "${DINGTALK_APP_SECRET}",
            "robotCode": "${DINGTALK_ROBOT_CODE}",
            "connectionMode": "stream",
            "allowedGroups": [
                "酒店管理群"
            ],
            "activationMode": "mention",
            "dmPolicy": "pairing"
        }
    },
    "skills": {
        "load": {
            "watch": true,
            "watchDebounceMs": 250
        }
    },
    "tools": {
        "browser": {
            "enabled": false
        },
        "camera": {
            "enabled": false
        }
    },
    "server": {
        "port": 3001
    }
}